@model List<BookMySpace.Models.User>

<h2>Users</h2>

<table border="1" cellpadding="8" cellspacing="0">
    <thead>
    <tr>
        <th>UserId</th>
        <th>FullName</th>
        <th>Email</th>
        <th>Phone</th>
        <th>RoleId</th>
        <th>IsActive</th>
        <th>CreatedAt</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var u in Model)
    {
        <tr>
            <td>@u.UserId</td>
            <td>@u.FullName</td>
            <td>@u.Email</td>
            <td>@u.Phone</td>
            <td>@u.RoleId</td>
            <td>@u.IsActive</td>
            <td>@u.CreatedAt</td>
        </tr>
    }
    </tbody>
</table>