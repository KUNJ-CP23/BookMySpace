@model List<BookMySpace.Models.Review>

<h2>Reviews</h2>

<table border="1" cellpadding="8" cellspacing="0">
    <thead>
    <tr>
        <th>ReviewId</th>
        <th>FacilityId</th>
        <th>UserId</th>
        <th>Rating</th>
        <th>Comment</th>
        <th>CreatedAt</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var r in Model)
    {
        <tr>
            <td>@r.ReviewId</td>
            <td>@r.FacilityId</td>
            <td>@r.UserId</td>
            <td>@r.Rating</td>
            <td>@r.Comment</td>
            <td>@r.CreatedAt</td>
        </tr>
    }
    </tbody>
</table>