@model List<BookMySpace.Models.Booking>

<h2>Bookings</h2>

<table border="1" cellpadding="8" cellspacing="0">
    <thead>
    <tr>
        <th>BookingId</th>
        <th>FacilityId</th>
        <th>UserId</th>
        <th>StartDate</th>
        <th>EndDate</th>
        <th>StartTime</th>
        <th>EndTime</th>
        <th>TotalPrice</th>
        <th>BookingStatus</th>
        <th>CreatedAt</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var b in Model)
    {
        <tr>
            <td>@b.BookingId</td>
            <td>@b.FacilityId</td>
            <td>@b.UserId</td>
            <td>@b.StartDate</td>
            <td>@b.EndDate</td>
            <td>@b.StartTime</td>
            <td>@b.EndTime</td>
            <td>@b.TotalPrice</td>
            <td>@b.BookingStatus</td>
            <td>@b.CreatedAt</td>
        </tr>
    }
    </tbody>
</table>